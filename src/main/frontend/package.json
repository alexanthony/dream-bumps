{
  "name": "dream-bumps",
  "version": "0.1.0",
  "description": "Definitely not fantasy bumps...",
  "engines": {
    "npm": ">=3"
  },
  "scripts": {
    "preinstall": "node tools/nodeVersionCheck.js",
    "setup": "node tools/setup/setupMessage.js && npm install && node tools/setup/setup.js",
    "remove-demo": "babel-node tools/removeDemo.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --hot --inline --content-base ./target --config ./webpack.config.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean-dist && npm run test",
    "build": "babel-node tools/build.js && npm run open:dist",
    "test": "jest",
    "test:CI": "babel-node tools/testCi.js",
    "test:cover": "npm run test -- --coverage ",
    "test:cover:CI": "npm run test:CI -- --coverage && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "npm run test -- --watch",
    "open:cover": "npm run test:cover && open coverage/lcov-report/index.html",
    "analyze-bundle": "babel-node ./tools/analyzeBundle.js"
  },
  "author": "Alex Anthony",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.14.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "chart.js": "^2.5.0",
    "classnames": "^2.2.5",
    "copy-webpack-plugin": "^4.0.1",
    "enzyme-to-json": "^1.5.1",
    "flexboxgrid": "^6.3.1",
    "flow": "^0.2.3",
    "flow-bin": "^0.42.0",
    "flow-typed": "^2.0.0",
    "jwt-decode": "^2.2.0",
    "material-grid": "^0.1.0",
    "material-ui": "^0.17.0",
    "material-ui-superselectfield": "^1.5.1",
    "object-assign": "4.1.0",
    "proxy-middleware": "^0.15.0",
    "react": "15.4.1",
    "react-chartjs-2": "^2.0.5",
    "react-dom": "15.4.1",
    "react-flexbox-grid": "^0.10.2",
    "react-redux": "5.0.1",
    "react-responsive": "^1.3.0",
    "react-router": "3.0.0",
    "react-router-redux": "4.0.7",
    "react-select": "^1.0.0-rc.3",
    "react-sortable-hoc": "^0.6.1",
    "react-tap-event-plugin": "^2.0.1",
    "react-timestamp": "^3.4.0",
    "redux": "3.6.0",
    "redux-auth-wrapper": "^0.9.0",
    "redux-form": "^6.4.3",
    "redux-form-material-ui": "^4.1.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "2.1.0",
    "webpack-dev-server": "^2.4.1",
    "webpack-merge": "^3.0.0"
  },
  "devDependencies": {
    "autoprefixer": "6.5.4",
    "babel-cli": "6.18.0",
    "babel-core": "6.20.0",
    "babel-eslint": "7.1.1",
    "babel-jest": "18.0.0",
    "babel-loader": "6.2.10",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-react-remove-prop-types": "0.2.11",
    "babel-polyfill": "6.20.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-latest": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.16.0",
    "browser-sync": "2.18.5",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "coveralls": "2.11.15",
    "css-loader": "^0.26.2",
    "enzyme": "2.6.0",
    "eslint": "^3.16.1",
    "eslint-config-prettier": "^2.1.1",
    "eslint-plugin-flowtype": "^2.21.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^2.0.0 || ^3.0.0 || ^4.0.0",
    "eslint-plugin-prettier": "^2.0.1",
    "eslint-plugin-react": "^6.4.1",
    "eslint-watch": "2.1.14",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.24.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "18.1.0",
    "json-loader": "0.5.4",
    "mockdate": "2.0.1",
    "node-sass": "4.0.0",
    "npm-run-all": "3.1.2",
    "open": "0.0.5",
    "postcss-loader": "1.2.1",
    "prettier": "^1.3.1",
    "prompt": "1.0.0",
    "react-addons-test-utils": "15.4.1",
    "redux-immutable-state-invariant": "1.2.4",
    "replace": "0.3.0",
    "rimraf": "2.5.4",
    "sass-loader": "6.0.2",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "2.2.1",
    "webpack-bundle-analyzer": "2.1.1",
    "webpack-dev-middleware": "1.9.0",
    "webpack-hot-middleware": "2.13.2",
    "webpack-md5-hash": "0.0.5"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy",
      "^.+\\.(gif|ttf|eot|svg|woff|woff2|ico|jpg)$": "<rootDir>/tools/fileMock.js"
    },
    "setupFiles": [
      "./src/testutils/jestsetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
